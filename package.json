{
    "name": "@pektin/client",
    "version": "1.0.991",
    "description": "A client to facilitate the control of pektin dns servers",
    "main": "dist/js/index.js",
    "types": "src/index.ts",
    "license": "MIT",
    "type": "module",
    "author": {
        "name": "Paul Colin Hennig",
        "url": "https://y.gy",
        "email": "pektin@y.gy"
    },
    "keywords": [
        "pektin",
        "dns",
        "server",
        "client",
        "domain",
        "name",
        "system",
        "install"
    ],
    "devDependencies": {
        "@types/node": "^16.11.12",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@pektin/config": "^1.0.1",
        "cross-fetch": "^3.1.4"
    },
    "scripts": {
        "build": "tsc",
        "start": "tsc --watch",
        "compile-policies": "tsc src/ribston-policies/* --outDir dist/ribston-policies --target es2021 --strict",
        "before-publish": "yarn build; yarn compile-policies; yarn compile-deno",
        "manual-test": "RED='\u001b[1;91m' ; NO_COLOR='\u001b[0m' ; echo \"${RED}Don't forget to compile the TS files first with: yarn start ${NO_COLOR}\" ; node dist/js/.manualTests/index.js",
        "compile-deno": "sh scripts/compile-deno.sh",
        "upgrade-config": "yarn upgrade --latest @pektin/config"
    }
}
